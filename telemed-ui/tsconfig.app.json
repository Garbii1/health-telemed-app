  "compilerOptions": {
    "outDir": "./out-tsc/app", // Output directory for this config
    "types": [] // Specify types if needed (e.g., "node")
  },
  "files": [
    "src/main.ts", // Entry point
    "src/polyfills.ts" // Polyfills (path may differ in newer Angular versions)
  ],
  "include": [
    "src/**/*.d.ts" // Include declaration files
  ]
}